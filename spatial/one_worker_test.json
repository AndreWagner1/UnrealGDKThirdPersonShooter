{
  "template": "small_bridge_v2",
  "world": {
    "dimensions": {
      "x_meters": 500,
      "z_meters": 500
    },
    "chunk_edge_length_meters": 20,
    "streaming_query_interval": 4,
    "legacy_flags": [
      {
        "name": "streaming_query_diff",
        "value": "true"
      },
      {
        "name": "bridge_qos_max_timeout",
        "value": "0"
      },
      {
        "name": "qos_max_unacked_pings_rate",
        "value": "10"
      },
      {
        "name": "enable_bridge_shunt_network_stack",
        "value": "true"
      },
      {
        "name": "enable_receptionist_v2",
        "value": "true"
      }
    ],
    "snapshots": {
      "snapshot_write_period_seconds": 0
    }
  },
  "workers": [
    {
      "worker_type": "UnrealWorker",
      "flags" : [],
      "load_balancing": {
        "points_of_interest": {
          "num_workers": 1,
          "points" : [
            {
              "x": 0,
              "z": 0
            }
          ]
        }
      },
      "permissions": [
        {
          "all": {}
        }
      ]
    },
    {
      "worker_type": "UnrealClient",
      "permissions": [
        {
          "all": {}
        }
      ]
    }
  ]
}
